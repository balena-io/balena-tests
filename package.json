{
  "name": "resinos-tests",
  "version": "1.0.2",
  "description": "Automated testing for ResinOS",
  "scripts": {
    "tslint": "tslint -t stylish \"lib/**/*.ts\"",
    "lint": "npm run tslint",
    "test": "npm run lint",
    "start": "ava-ts"
  },
  "repository": {
    "type": "git",
    "url": "git+https://github.com/resin-io/resinos-tests.git"
  },
  "keywords": [
    "resinOS",
    "testing",
    "resin.io",
    "resin"
  ],
  "author": "Resin Inc. <hello@etcher.io>",
  "license": "Apache-2.0",
  "bugs": {
    "url": "https://github.com/resin-io/resinos-tests/issues"
  },
  "homepage": "https://github.com/resin-io/resinos-tests",
  "ava": {
    "files": [
      "lib/main.ts"
    ],
    "failFast": true,
    "failWithoutAssertions": true
  },
  "dependencies": {
    "ava": "^0.23.0",
    "ava-ts": "^0.23.0",
    "bluebird": "^3.5.0",
    "drivelist": "^5.0.22",
    "etcher-image-write": "^9.1.3",
    "fs-extra": "^3.0.1",
    "mountutils": "^1.2.0",
    "nodegit": "^0.18.3",
    "progress-stream": "^2.0.0",
    "resin-cli-visuals": "^1.4.0",
    "resin-image-fs": "^4.0.2",
    "resin-sdk": "^7.0.0",
    "ts-node": "^3.3.0",
    "typescript": "^2.6.1"
  },
  "devDependencies": {
    "tslint": "^5.8.0"
  }
}
